{
  "auth": {
    "profiles": {
      "anthropic:default": {
        "provider": "anthropic",
        "mode": "token",
        "token": "{{ lookup('env', 'CLAUDE_CODE_OAUTH_TOKEN') }}"
      }
    }
  },
  "gateway": {
    "mode": "local",
    "auth": {
      "mode": "token",
      "token": "{{ lookup('env', 'MOLTBOT_GATEWAY_TOKEN') }}"
    },
    "trustedProxies": ["10.3.32.1"],
    "controlUi": {
      "allowInsecureAuth": true
    }
  },
  "channels": {
    "telegram": {
      "enabled": true
    }
  }
}
