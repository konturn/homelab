{
  "version": 1,
  "profiles": {
    "anthropic:manual": {
      "type": "token",
      "provider": "anthropic",
      "token": "{{ lookup('env', 'CLAUDE_CODE_OAUTH_TOKEN') }}"
    }{% if vault_claude_fallback_token | default('') %},
    "anthropic:fallback": {
      "type": "token",
      "provider": "anthropic",
      "token": "{{ vault_claude_fallback_token }}"
    }{% endif %}
  }
}
